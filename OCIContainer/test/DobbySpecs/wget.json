{
    "version": "1.0.0",
    "cwd": "/",
    "args": [
        "wget",
        "example.com"
    ],
    "env": [

    ],
    "user": {
        "uid": 1000,
        "gid": 1000
    },
    "console": {
        "limit": 65536,
        "path": "/tmp/wget.log"
    },
    "etc": {
        "group": [
            "root:x:0:"
        ],
        "passwd": [
            "root::0:0:root:/:/bin/false"
        ]
    },
    "memLimit": 41943040,
    "network": "nat",
    "mounts": [{
        "destination": "/etc/resolv.conf",
        "type": "bind",
        "source": "/etc/resolv.conf",
        "options": [
            "ro",
            "rbind",
            "rprivate",
            "nosuid",
            "noexec",
            "nodev"
        ]
    }]
}